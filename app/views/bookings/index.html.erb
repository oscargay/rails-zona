<h1>Bookings To Check</h1>

<ol>
  <% @bookings_tocheck.each do |booking|  %>
  <li><%= booking.start_date %> || <%= booking.end_date %> || <%= booking.status%></li>
  <h3><%=link_to booking.equipment.name, booking_path(booking)%></h3>
  <% end %>
</ol>

<hr>

